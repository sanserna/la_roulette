// - Base imports
@import 'base/_base_variables'
@import 'base/_base_mixins'



// -------------------
// @FUNDADOR
// -------------------

.fundador {
    padding 10px 0
    .fundador__img {
        border-radius 3px
        box-shadow 0 2px 2px rgba(black,.5)
        width 100%
    }
    .fundador__name {
        color $yellow-logo
        padding 5px 0 2px 0
    }
    .fundador__pro {
        color white
        text-align center
    }
}