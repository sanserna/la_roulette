{
  "rules": {
    "camelcase": 0,
    "curly": 1,
    "eqeqeq": 0,
    "indent": [2, 4],
    "max-len": 0,
    "max-nested-callbacks": 0,
    "no-alert": 0,
    "no-empty": 1,
    "no-multiple-empty-lines": 0,
    "no-nested-ternary": 1,
    "no-obj-calls": 2,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "one-var": [2, "always"],
    "padded-blocks": [2, "always"],
    "quotes": 0,
    "require-jsdoc": 0,
    "semi": 1,
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [2, "always"],
    "wrap-iife": [2, "outside"]
  }
}
